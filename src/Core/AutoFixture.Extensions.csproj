<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <ProjectGuid>{2D00E58A-FEF1-4CB5-8807-346FF08F6EF6}</ProjectGuid>
    <AssemblyName>AutoFixture.Extensions</AssemblyName>
    <RootNamespace>AutoFixture.Extensions</RootNamespace>
    <TargetFrameworks>net6.0;net5.0</TargetFrameworks>
    <Platforms>AnyCPU</Platforms>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>AutoMoqFixture.Ext</PackageId>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Authors>alex.soh</Authors>
    <Description>An [AutoFixture](https://github.com/AutoFixture/AutoFixture) customization which uses Moq for auto mocking objects.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/takato1314/automoqfixture_ext</PackageProjectUrl>
    <RepositoryUrl>https://github.com/takato1314/automoqfixture_ext</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AutoMapper" Version="11.0.0" />
    <PackageReference Include="Ensure.That" Version="10.1.0" />
    <PackageReference Include="FluentAssertions" Version="6.3.0" />
    <PackageReference Include="GitVersion.MsBuild" Version="5.8.1">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="6.0.0" />
    <PackageReference Include="AutoFixture.AutoMoq" Version="4.17.0" />
    <PackageReference Include="AutoFixture.Xunit2" Version="4.17.0" />
    <PackageReference Include="System.ComponentModel.TypeConverter" Version="4.3.0" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="SonarAnalyzer.CSharp" Version="8.35.0.42613" />
  </ItemGroup>

</Project>
